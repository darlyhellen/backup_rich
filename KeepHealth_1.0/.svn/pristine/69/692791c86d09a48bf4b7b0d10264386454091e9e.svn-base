<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/bggray" >

    <LinearLayout
        android:id="@+id/id_ll1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/home_bg"
        android:orientation="vertical" >

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:padding="14dp"
            android:text="帮忙医"
            android:textColor="#ffffffff"
            android:textSize="@dimen/textview_large" />
    </LinearLayout>

    <com.ytdinfo.keephealth.ui.view.MyAdGallery
        android:id="@+id/id_iv_private_doc"
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="21dp"
        android:layout_marginTop="13dp"
        android:visibility="visible" /> 
     <LinearLayout
        android:id="@+id/ovalLayout"
        android:layout_width="match_parent"
        android:layout_height="15dp"
        android:layout_below="@id/id_iv_private_doc"
        android:layout_marginTop="-40dp"
        android:gravity="center"
        android:orientation="horizontal" >
    </LinearLayout>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_above="@id/id_iv_private_doc"
        android:layout_below="@id/id_ll1"
        android:orientation="vertical" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="19"
            android:background="@color/bgwhite"
            android:orientation="vertical" >

            <com.ytdinfo.keephealth.ui.HomeItem_1View
                android:id="@+id/id_homeItem_1View_1"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1" />

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:layout_marginLeft="20dp"
                android:layout_marginRight="20dp"
                android:background="@color/divider" />

            <com.ytdinfo.keephealth.ui.HomeItem_1View
                android:id="@+id/id_homeItem_1View_2"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1" />

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="@color/divider" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_marginTop="12dp"
            android:layout_weight="20"
            android:background="@color/bgwhite"
            android:orientation="vertical" >

            <TextView
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:gravity="center"
                android:text="体检专区"
                android:textColor="@color/black"
                android:textSize="@dimen/textview_general" />

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="@color/divider" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:gravity="center_vertical"
                android:orientation="horizontal" >

                <com.ytdinfo.keephealth.ui.HomeItem_2View
                    android:id="@+id/id_homeItem_2View_1"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:background="@drawable/vertical_line" />

                <com.ytdinfo.keephealth.ui.HomeItem_2View
                    android:id="@+id/id_homeItem_2View_2"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1" />
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="@color/divider" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_gravity="center_vertical"
                android:layout_weight="1"
                android:orientation="horizontal" >

                <com.ytdinfo.keephealth.ui.HomeItem_2View
                    android:id="@+id/id_homeItem_2View_3"
                    android:layout_width="0dp"
                    android:layout_gravity="center_vertical"
                    android:layout_height="wrap_content"
                    android:layout_weight="1" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:background="@drawable/vertical_line" />

                <com.ytdinfo.keephealth.ui.HomeItem_2View
                    android:id="@+id/id_homeItem_2View_4"
                    android:layout_gravity="center_vertical"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1" />
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="@color/divider" />
        </LinearLayout>
    </LinearLayout>

</RelativeLayout>