package com.ytdinfo.keephealth.zhangyuhui.model;

import java.util.ArrayList;

public class IAOrganization {
	public String organization_name;
	public int organization_id;
	public int organization_floor_num;
	public boolean organization_hasstate;
	public ArrayList<IAOrganization_Departments> organization_departments;

	public IAOrganization(String organization_name, int organization_id,
			boolean organization_hasstate,
			ArrayList<IAOrganization_Departments> organization_departments) {
		super();
		this.organization_name = organization_name;
		this.organization_id = organization_id;
		this.organization_hasstate = organization_hasstate;
		this.organization_departments = organization_departments;
	}

	public IAOrganization(String organization_name, int organization_id) {
		super();
		this.organization_name = organization_name;
		this.organization_id = organization_id;
	}

	public IAOrganization() {
		// TODO Auto-generated constructor stub
	}

	public String getOrganization_name() {
		return organization_name;
	}

	public void setOrganization_name(String organization_name) {
		this.organization_name = organization_name;
	}

	public int getOrganization_id() {
		return organization_id;
	}

	public void setOrganization_id(int organization_id) {
		this.organization_id = organization_id;
	}

	public int getOrganization_floor_num() {
		return organization_floor_num;
	}

	public void setOrganization_floor_num(int organization_floor_num) {
		this.organization_floor_num = organization_floor_num;
	}

	public boolean isOrganization_hasstate() {
		return organization_hasstate;
	}

	public void setOrganization_hasstate(boolean organization_hasstate) {
		this.organization_hasstate = organization_hasstate;
	}

	public ArrayList<IAOrganization_Departments> getOrganization_departments() {
		return organization_departments;
	}

	public void setOrganization_departments(
			ArrayList<IAOrganization_Departments> organization_departments) {
		this.organization_departments = organization_departments;
	}

}
